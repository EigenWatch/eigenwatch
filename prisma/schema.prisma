  total_volume_usd    Decimal?       @db.Decimal
  timestamp           DateTime       @db.Timestamp(6)
  source              String?        @db.VarChar
  is_proxy_price      Boolean?
  proxy_token_address String?        @db.VarChar
  proxy_token_symbol  String?        @db.VarChar
  token_metadata      token_metadata @relation(fields: [token_address], references: [contract_address], onDelete: Cascade, onUpdate: NoAction)

  @@unique([token_address, timestamp], map: "uq_token_price_ts")
  @@index([timestamp], map: "idx_token_price_date")
  @@index([token_address, timestamp], map: "idx_token_price_ts")
  @@index([timestamp], map: "ix_token_price_history_timestamp")
  @@index([token_address], map: "ix_token_price_history_token_address")
}

model global_rewards_config {
  id                   Int      @id @default(autoincrement())
  config_type          String   @unique @db.VarChar
  current_value        BigInt
  current_set_at_block Int
  current_set_at       DateTime @db.Timestamp(6)
  previous_value       BigInt?
  created_at           DateTime @default(now()) @db.Timestamp(6)
  updated_at           DateTime @default(now()) @db.Timestamp(6)

  @@index([config_type], map: "idx_global_config_type")
}

model operator_set_state {
  id                     String    @id @db.VarChar
  avs_id                 String    @db.VarChar
  operator_set_id        Int
  active_operator_count  Int?
  total_allocation_count Int?
  total_allocated_usd    Decimal?  @db.Decimal
  supported_strategies   String[]  @db.VarChar
  current_split_bips     Int?
  created_at_block       Int?
  last_activity_at       DateTime? @db.Timestamp(6)
  created_at             DateTime  @default(now()) @db.Timestamp(6)
  updated_at             DateTime  @default(now()) @db.Timestamp(6)
  avs                    avs       @relation(fields: [avs_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@index([avs_id], map: "idx_operator_set_state_avs")
  @@index([total_allocated_usd], map: "idx_operator_set_state_usd")
  @@index([avs_id], map: "ix_operator_set_state_avs_id")
}

model token_price_proxies {
  token_address       String    @id @db.VarChar
  token_symbol        String    @db.VarChar
  proxy_token_address String    @db.VarChar
  proxy_token_symbol  String    @db.VarChar
  price_multiplier    Decimal?  @db.Decimal
  relationship_type   String    @db.VarChar
  reason              String?   @db.VarChar
  is_active           Boolean?
  last_used_at        DateTime? @db.Timestamp(6)
  created_at          DateTime  @default(now()) @db.Timestamp(6)
  updated_at          DateTime  @default(now()) @db.Timestamp(6)

  @@index([is_active], map: "idx_price_proxy_active")
  @@index([relationship_type], map: "idx_price_proxy_type")
}
